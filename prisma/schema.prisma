generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model assisstant {
  id              Int      @id @default(autoincrement())
  assisstant_code String   @db.VarChar(3)
  name            String   @db.VarChar(255)
  time            DateTime @db.DateTime(0)
}
